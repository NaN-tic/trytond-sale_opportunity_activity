<data>
    <xpath
        expr="/form/notebook"
        position="inside">
        <page string="Activities" id="activities">
            <field name="activities" colspan="4"/>
        </page>
    </xpath>

    <xpath
        expr="/form/notebook/page[@id='opportunity']/field[@name='end_date']"
        position="after">
        <label name="last_action_date"/>
        <field name="last_action_date"/>
    </xpath>

</data>
